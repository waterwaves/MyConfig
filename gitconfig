[color]
	ui = true
[diff]
	algorithm = patience
[rerere]
	enabled = true
[push]
	default = current
[core]
	editor = vim
  excludesfile = ~/MyConfig/gitignore
[alias]
  co = checkout
  br = branch
  st = status
  sb = status -s -b
  logs = log --stat=120,120 -p
  diffm = diff --stat=120,120 -p origin/master...
  l = log --oneline --graph --decorate
  g = grep -En
  ci = commit -v
  ap = add -p
  rebase2 = rebase HEAD~2 -i
  fixup = commit -am 'fixup'

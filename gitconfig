[color]
	ui = true
[diff]
	algorithm = patience
[rerere]
	enabled = true
[push]
	default = current
[core]
	editor = /usr/bin/vim
[alias]
	co = checkout
	br = branch
	st = status
	sb = status -s -b
	logs = log --stat=120,120 -p
	diffm = diff --stat=120,120 -p origin/master...
	l = log --oneline --graph --decorate
	g = grep -En
